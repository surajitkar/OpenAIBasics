{
  "all": true,
  "include": [
    "**/*.js"
  ],
  "exclude": [
    "tests/**",
    "node_modules/**",
    "coverage/**",
    "*.config.js",
    "*.test.js",
    "**/*.test.js",
    "demo-all.js"
  ],
  "reporter": [
    "text",
    "html",
    "json-summary",
    "lcov"
  ],
  "reports-dir": "coverage",
  "temp-directory": ".nyc_output",
  "check-coverage": true,
  "lines": 75,
  "functions": 75,
  "branches": 70,
  "statements": 75,
  "per-file": true,
  "skip-full": false,
  "clean": true
}