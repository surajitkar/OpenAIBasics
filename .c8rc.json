{
  "all": true,
  "src": [
    "foundations",
    "chatbot",
    "advanced",
    "agents",
    "assistants",
    "index.js",
    "demo-all.js"
  ],
  "exclude": [
    "tests/**",
    "node_modules/**",
    "coverage/**",
    "*.config.js",
    "*.config.mjs"
  ],
  "reporter": [
    "text",
    "html",
    "json-summary",
    "lcov"
  ],
  "reports-dir": "coverage",
  "temp-directory": ".nyc_output",
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "per-file": true,
  "clean": true,
  "skip-full": false
}